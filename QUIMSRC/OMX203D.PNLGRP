          VARS='FMINTL'.


.*   -------------------------------------------------------------
.*   Conditions
.*   -------------------------------------------------------------

.*        --------------------------------------------------
.*        Condition for add/change/display
.*        --------------------------------------------------
:COND     NAME=CODSP
          EXPR='FMIDSP  = "1"'
          EVAL=ALWAYS.

:COND     NAME=CONODSP
          EXPR='FMIDSP г= "1"'
          EVAL=ALWAYS.

:COND     NAME=COADD
          EXPR='FMIADD  = "1"'
          EVAL=ALWAYS.

:COND     NAME=CONOADD
          EXPR='FMIADD г= "1"'
          EVAL=ALWAYS.

.*        --------------------------------------------------
.*        Condition for F11=View
.*        --------------------------------------------------
:COND     NAME=COF11V1
          EXPR='FMVIEW=2'
          EXPR='*AND'
          EXPR='FMCSUL="9"'
          EVAL=ALWAYS.

:COND     NAME=COF11V2
          EXPR='FMVIEW=0'
          EXPR='*AND'
          EXPR='FMCSUL="9"'
          EVAL=ALWAYS.

:COND     NAME=COF11V3
          EXPR='FMVIEW=1'
          EXPR='*AND'
          EXPR='FMCSUL="9"'
          EVAL=ALWAYS.

.*        --------------------------------------------------
.*        Condition for F10=View
.*        --------------------------------------------------
:COND     NAME=COF10V1
          EXPR='FMVIEW г= 3'
          EVAL=ALWAYS.

:COND     NAME=COF10V2
          EXPR='FMVIEW  = 3'
          EVAL=ALWAYS.

.*        --------------------------------------------------
.*        Condition the OMS manager.
.*        --------------------------------------------------
:COND     NAME=COMANAGER
          EXPR='FMCSUL="9"'
          EVAL=ALWAYS.

.*        иииииииииииииииииииииииииииииииииииииииииииииииииии
.*        Condition for date or time display
.*        иииииииииииииииииииииииииииииииииииииииииииииииииии
:COND     NAME=CODATE
          EXPR='FMDOTC="1"'
          EVAL=ALWAYS.

:COND     NAME=COTIME
          EXPR='FMDOTC="0"'
          EVAL=ALWAYS.


