:TI       VALUE='"000000"'.&MSG(UNK0001).
:ETL.
:ECLASS.

.*        --------------------------------------------------
.*        Dialog
.*        --------------------------------------------------
:CLASS    NAME=DIALOG
          BASETYPE='BIN 15'.
:ECLASS.

.*        --------------------------------------------------
.*        Indicator
.*        --------------------------------------------------
:CLASS    NAME=INDI
          CASE=UPPER
          WIDTH=4
          BASETYPE='CHAR 1'.
:TL.
:TI       VALUE='"0"'.*NO
:TI       VALUE='"1"'.*YES
:TI       VALUE='" "'.*YES
:ETL.
:ECLASS.

.*        --------------------------------------------------
.*        Program description
.*        --------------------------------------------------
:CLASS    NAME=PGMD
          BASETYPE='CHAR 30'.
:ECLASS.

.*        --------------------------------------------------
.*        Format code
.*        --------------------------------------------------
:CLASS    NAME=FMTC
          BASETYPE='OBJNAME 10'.
:ECLASS.

.*        --------------------------------------------------
.*        User option
.*        --------------------------------------------------
:CLASS    NAME=UOPC
          BASETYPE='CHAR 2'
          BLANKS=IGNORE
          CASE=UPPER.
:ECLASS.

.*        --------------------------------------------------
.*        User option description
.*        --------------------------------------------------
:CLASS    NAME=UOPD
          BASETYPE='CHAR 30'.
:ECLASS.

.*        --------------------------------------------------
.*        Support level
.*        --------------------------------------------------
:CLASS    NAME=SULN
          BASETYPE='CHAR 1'.
:ECLASS.

.*        --------------------------------------------------
.*        Program name
.*        --------------------------------------------------
:CLASS    NAME=PGMN
          BASETYPE='OBJNAME 10'.
:ECLASS.

.*        --------------------------------------------------
.*        Program library
.*        --------------------------------------------------
:CLASS    NAME=PGML
          BASETYPE='OBJNAME 10'.
:TL.
:TI       VALUE='"*LIBL     "'.*LIBL
:ETL.
:ECLASS.

.*        --------------------------------------------------
.*        Pass parameter indicator
.*        --------------------------------------------------
:CLASS    NAME=PRMI
          WIDTH=1
          BASETYPE='CHAR 1'.
:ECLASS.

.*        --------------------------------------------------
.*        Interface level.
.*        --------------------------------------------------
:CLASS    NAME=INTL
          BASETYPE='CHAR 8'
